{
  "name": "@y/protocols",
  "version": "1.0.6-rc.1",
  "description": "Yjs encoding protocols",
  "type": "module",
  "funding": {
    "type": "GitHub Sponsors ‚ù§",
    "url": "https://github.com/sponsors/dmonad"
  },
  "files": [
    "types/**/*.d.ts",
    "!types/**/*test.d.ts",
    "src/**/*.js",
    "!src/**/*test.js"
  ],
  "scripts": {
    "clean": "rm -rf dist types",
    "dist": "npm run clean && npm run types",
    "test": "node --expose-gc src/test.js && npm run lint",
    "debug": "node --inspect-brk --expose-gc src/test.js",
    "lint": "standard && tsc --skipLibCheck",
    "types": "tsc --skipLibCheck",
    "preversion": "npm run dist && npm run types",
    "postpublish": "npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yjs/y-protocols.git"
  },
  "keywords": [
    "Yjs"
  ],
  "author": "Kevin Jahns <kevin.jahns@protonmail.com>",
  "license": "MIT",
  "standard": {
    "ignore": [
      "/dist",
      "/node_modules"
    ]
  },
  "bugs": {
    "url": "https://github.com/yjs/y-protocols/issues"
  },
  "homepage": "https://github.com/yjs/y-protocols#readme",
  "exports": {
    "./sync": {
      "types": "./types/sync.d.ts",
      "default": "./src/sync.js"
    },
    "./awareness": {
      "types": "./types/awareness.d.ts",
      "default": "./src/awareness.js"
    },
    "./auth": {
      "types": "./types/auth.d.ts",
      "default": "./src/auth.js"
    }
  },
  "dependencies": {
    "lib0": "^1.0.0-rc.1"
  },
  "devDependencies": {
    "@types/node": "^20.6.2",
    "@y/y": "^14.0.0-16",
    "concurrently": "^5.3.0",
    "standard": "^17.1.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@y/y": "*"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  }
}
